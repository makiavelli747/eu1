		enemies = 0

		# 100 = max trader rate, lesser means slower focus on sending a trader.
		traders = 100

		# 100 = Total monopolist, will refuse trade as much as possible.
		monopoly = 25

		# 100 = Total warmonger, 0 = absolute pacifist
		war = 100
 
 

